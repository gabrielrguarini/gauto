generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                Int       @id @default(autoincrement())
  email             String    @unique
  senha             String
  dataDeCriacao     DateTime? @default(now())
  dataDeAtualizacao DateTime? @default(now()) @updatedAt
}
